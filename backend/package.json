{
  "name": "nft_erc721_backend",
  "version": "0.0.1",
  "author": "PhilipMantrov",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "protoc": "cd ../ && buf generate",
    "build-contract": "npx hardhat compile",
    "build": "webpack",
    "build:gateway": "cd grpc-gateway/nft && gox -osarch=\"linux/amd64 windows/amd64 darwin/arm64 darwin/amd64\" -output=\"../../bundle/bin/{{.Dir}}_gateway_{{.OS}}_{{.Arch}}\"",
    "start:gateway:mac_arm": "bundle/bin/nft_gateway_darwin_arm64",
    "start:gateway:mac": "bundle/bin/nft_gateway_darwin_amd64",
    "start:gateway:linux": "bundle/bin/nft_gateway_linux_amd64",
    "start:gateway:win": "bundle/bin/nft_gateway_windows_amd64.exe",
    "start:backend": "run-p start:backend:* --print-label",
    "start:backend:nft_watcher": "cd bundle && node server.js nft.watcher.v1"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.9.3",
    "@grpc/proto-loader": "^0.7.9",
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/microservices": "^10.2.5",
    "@nestjs/platform-express": "^9.0.0",
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.2",
    "@nomicfoundation/hardhat-ethers": "^3.0.4",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.9",
    "@nomicfoundation/hardhat-toolbox": "^3.0.0",
    "@nomicfoundation/hardhat-verify": "^1.1.1",
    "@openzeppelin/contracts": "^4.9.3",
    "@typechain/ethers-v6": "^0.5.0",
    "@typechain/hardhat": "^9.0.0",
    "@types/mocha": "^10.0.1",
    "chai": "^4.3.8",
    "ethers": "^6.7.1",
    "hardhat": "^2.17.3",
    "hardhat-gas-reporter": "^1.0.9",
    "nestjs-grpc-reflection": "^0.0.21",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "solidity-coverage": "^0.8.4",
    "typechain": "^8.3.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "29.2.4",
    "@types/node": "18.11.18",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "child_process": "^1.0.2",
    "copy-webpack-plugin": "^11.0.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "hardhat": "^2.17.3",
    "jest": "29.3.1",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.1",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^4.7.4",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
